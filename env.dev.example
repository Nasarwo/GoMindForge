# .env - Переменные окружения для разработки
# Скопируйте этот файл и замените значения на реальные

# Сервер
PORT=8080
GIN_MODE=debug

# JWT секрет (ОБЯЗАТЕЛЬНО измените на случайную строку!)
JWT_SECRET=dev-secret-key-change-this-in-production-minimum-32-characters

# Логирование
LOG_LEVEL=debug
LOG_FORMAT=text

# AI провайдеры
OPENROUTER_API_KEY=your-openrouter-api-key-here

# Дополнительные настройки (опционально)
# DEEPSEEK_API_KEY=sk-your-deepseek-api-key-here
# GROK_API_KEY=your-grok-api-key-here
